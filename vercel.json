{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "builds": [{
        "use": "@vercel/node",
        "config": {
            "includeFiles": ["dist/**"]
        }
    }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js"
      }
    ]
}